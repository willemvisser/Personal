		<id>5</id> <devices> <device> <id>BREADBOARD_IR</id> <name>Breadboard D1</name> 
		<portAddress>D</portAddress> <port>1</port> <logicalPort>1</logicalPort> 
		<digital>true</digital> <type>Pump</type> </device> </devices> </xbee> -->
	<xbee>
		<address>00 13 A2 00 40 BE 47 03</address>
		<name>Pool Pump Controller</name>
		<id>8</id>
		<devices>
			<device>
				<id>PP_TEMP</id>
