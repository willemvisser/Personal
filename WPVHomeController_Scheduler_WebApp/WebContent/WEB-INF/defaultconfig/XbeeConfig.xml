				<portAddress>D</portAddress>
				<port>0</port>
				<logicalPort>0</logicalPort>
				<digital>false</digital>
				<type>Temperature</type>
			</device>
			<device>
				<id>PP_PUMP</id>
				<name>Pool Pump</name>
				<portAddress>D</portAddress>
				<port>5</port>
				<logicalPort>5</logicalPort>
				<digital>true</digital>
				<type>Pump</type>
