				<portAddress>D</portAddress>
				<port>5</port>
				<logicalPort>5</logicalPort>
				<digital>true</digital>
				<type>Light</type>
			</device>
		</devices>
	</xbee>
	<xbee>
		<address>00 13 A2 00 40 BE 46 EA</address>
		<name>Study Controller</name>
		<id>4</id>
		<devices>
			<device>
				<id>STUDY_LAMP</id>
				<name>Study Lamp</name>
				<portAddress>P</portAddress>
				<port>2</port>
				<logicalPort>12</logicalPort>
				<digital>true</digital>
				<type>Light</type>
			</device>
			<device>
				<id>STUDY_LED</id>
				<name>Study LED</name>
				<portAddress>P</portAddress>
				<port>1</port>
				<logicalPort>11</logicalPort>
				<digital>true</digital>
				<type>Light</type>
			</device>
			<device>
				<id>STUDY_TEMP</id>
				<name>Study Temperature</name>
				<portAddress>D</portAddress>
				<port>0</port>
				<logicalPort>0</logicalPort>
				<digital>false</digital>
				<type>Temperature</type>
			</device><!-- <device> <id>STUDY_IR</id> <name>Study IR</name> <portAddress>D</portAddress> 
				<port>1</port> <logicalPort>1</logicalPort> <digital>true</digital> <type>Pump</type> 
				</device> -->
			<device>
				<id>STUDY_S1</id>
				<name>Study LED Switch</name>
				<portAddress>D</portAddress>
				<port>1</port>
				<logicalPort>1</logicalPort>
				<digital>true</digital>
				<type>ToggleSwitch</type>
				<linkedDeviceId>STUDY_LED</linkedDeviceId>
			</device>
			<device>
				<id>STUDY_S2</id>
				<name>Study S2</name>
				<portAddress>D</portAddress>
				<port>2</port>
				<logicalPort>2</logicalPort>
				<digital>true</digital>
				<type>ToggleSwitch</type>
				<linkedDeviceId>STUDY_LAMP</linkedDeviceId>
			</device>			<!-- <device> <id>STUDY_IR</id> <name>Study Infrared</name> <portAddress>D</portAddress> 
				<port>1</port> <logicalPort>1</logicalPort> <digital>true</digital> <type>Infrared</type> 
				</device> -->
		</devices>
