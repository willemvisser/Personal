				<port>5</port>
				<logicalPort>5</logicalPort>
				<digital>true</digital>
				<type>Irrigation</type>
			</device>
		</devices>
	</xbee>
	<xbee>
		<address>00 13 A2 00 40 BE 46 EE</address>
		<name>Outside - Pump and Lights</name>
		<id>3</id>
		<devices>
			<device>
				<id>IRR_PUMP</id>
				<name>Irrigation Pump</name>
				<portAddress>D</portAddress>
				<port>7</port>
				<logicalPort>7</logicalPort>
				<digital>true</digital>
				<type>Pump</type>
			</device>
			<device>
				<id>LIGHT_PATH</id>
